            if ('false' === 'true') {
                pathLevel = ("" + pathLevel).replace(
                    /\.(?:html?|php[0-9]?|aspx?|cfg|py)$/i, '');
            }
            toSet["page_attr_url_path_" + level] = pathLevel;
        }
        level++;
    }
    toSet['page_attr_meta_keywords'] = Krux('scrape.meta_name', 'keywords');
    prefix = libUtil.resolvePrefix('none', 'undefined',
        'undefined');
    toSet = Krux('prefix:attr', toSet, prefix);
    domain = Krux('get', 'domain');
    comDomain = domain.match(/\.([^\.]+\.com)$/);
    if (comDomain) {
        domain = comDomain[1];
    }
    toSet['page_attr_domain'] = domain;
    Krux('set', toSet);
}).call();
</script></span><span class="kxtag kxinvisible" data-id="39874"><script>
