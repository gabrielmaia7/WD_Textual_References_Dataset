<script id="sentry-script" src="https://browser.sentry-cdn.com/6.3.0/bundle.tracing.min.js" integrity="sha384-bYnMk4rdUJ32XikJR2cXhgirS37DAde7poz0I7hPFWkmGzvhHs1TXBHlU8Gv+hQn" crossorigin="anonymous"></script>
<script>
if(Sentry) {
    Sentry.init({
        dsn:'https://ab7d56a3164b42538b5a3f2582dc2af3@o633.ingest.sentry.io/5950931',
        integrations: [new Sentry.Integrations.BrowserTracing()],
        tracesSampleRate: 0.0,
        beforeSend(event, hint) {
            // filter out UnhandledRejection errors likely thrown by browser extensions
            if (event && event.exception && event.exception.values && event.exception.values.length == 1) {
                var e = event.exception.values[0];
                if (e.type === "UnhandledRejection" && e.value.match(/Non-Error promise rejection captured with value: Object Not Found Matching Id/i)) {
                    return null;
                }
            }
            return event;
        },
    });
}
</script>


            
        

        
        
