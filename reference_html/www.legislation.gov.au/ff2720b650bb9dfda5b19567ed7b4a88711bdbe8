        function bookmarksite(title, url) {
            if (window.sidebar && window.sidebar.addPanel) // firefox
                window.sidebar.addPanel(title, url, "");
            else if (window.opera && window.print) { // opera
                var elem = document.createElement('a');
                elem.setAttribute('href', url);
                elem.setAttribute('title', title);
                elem.setAttribute('rel', 'sidebar');
                elem.click();
            }
            else if (window.external && ('AddFavorite' in window.external))// ie
                window.external.AddFavorite(url, title);
        }
        function DoSearch() {
            var searchtext = $find('ctl00_txtRegularSearch').get_value();
            location.href = "/Search/" + replacestring(searchtext);
            return false;
        }
        function replacestring(stringtoreplace) {
            stringtoreplace = stringtoreplace.split("&").join("$A$");

            stringtoreplace = stringtoreplace.split('*').join("$ST$");
            stringtoreplace = stringtoreplace.split("'").join("$SQ$");
            stringtoreplace = stringtoreplace.split('"').join("$DQ$");
            stringtoreplace = stringtoreplace.split("/").join("$FS$");
            stringtoreplace = stringtoreplace.split("\\").join("$BS$");
            stringtoreplace = stringtoreplace.split(",").join("$C$");
            stringtoreplace = stringtoreplace.split(";").join("$SC$");
            stringtoreplace = stringtoreplace.split("(").join("$LB$");
            stringtoreplace = stringtoreplace.split(")").join("$RB$");
            stringtoreplace = stringtoreplace.split("{").join("$LCB$");
            stringtoreplace = stringtoreplace.split("}").join("$RCB$");
            stringtoreplace = stringtoreplace.split(":").join("$CO$");
            stringtoreplace = stringtoreplace.split("#").join("$HA$");
            stringtoreplace = stringtoreplace.split("+").join("$PL$");

            return stringtoreplace
        }

        function forceLoginout(eventTarget, eventArgument) {
            if (!theForm.onsubmit || (theForm.onsubmit() != false)) {
                theForm.action = "/MyAccount";
            }
        }
    </script>
    
