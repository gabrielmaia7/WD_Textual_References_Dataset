    <script type="text/javascript">
        $(function onLoaded(sender, eventArgs) {
            
            var urllocation = location.href; //find url parameter
            if (urllocation.indexOf("#") > -1) {
                
                //var headerContainerHeight = $('.header-container').outerHeight(true);
                //var headroomHeight = $('.headroom headroom--not-top').outerHeight(true);
                window.location.hash = self.document.location.hash;
                //alert(headerContainerHeight);
                //alert(headroomHeight);
                $(window).scrollTop($(window).scrollTop() - 150);
                
            }
        });
    </script>



