                    <div id="MainContent_pnlForTOC">
	

<script type="text/javascript">
    
    function SetSelectedNode(event) {
        var node = event.srcElement || event.target;
        var COOKIE_NAME_SELECTED_NODE_ID = document.getElementById("MainContent_ctl17_lblComlawID").firstChild.data + "SelectedNodeID"; //Get a unique cookie name based on ComlawID
        if (node.id != "") {
            setCookie(COOKIE_NAME_SELECTED_NODE_ID, node.id); //handles if user clicks on the text
        }
        else {
            setCookie(COOKIE_NAME_SELECTED_NODE_ID, node.parentNode.id); //handles if the user clicks on the '+' or '-' image
        }
    }

    function pageLoad() {
        var COOKIE_NAME_SELECTED_NODE_ID = document.getElementById("MainContent_ctl17_lblComlawID").firstChild.data + "SelectedNodeID"; //Get a unique cookie name based on ComlawID
        var COOKIE_NAME_EXPAND_ALL = document.getElementById("MainContent_ctl17_lblComlawID").firstChild.data + "ExpandAll"; //Get a unique cookie name based on ComlawID

        var expandAll = getCookie(COOKIE_NAME_EXPAND_ALL);
        if (expandAll == "true") {
            TreeviewExpandCollapseAll("MainContent_ctl17_trTOC", true);
        }

        var selectedNodeID = getCookie(COOKIE_NAME_SELECTED_NODE_ID);
        if (selectedNodeID != null && selectedNodeID != "") {
            TreeviewExpandNode("MainContent_ctl17_trTOC", selectedNodeID);
        }

        if (expandAll == "true") {
            ScrollIntoSelectedNode(); 
        }
     }

     function ScrollIntoSelectedNode() {
         var COOKIE_NAME_SELECTED_NODE_ID = document.getElementById("MainContent_ctl17_lblComlawID").firstChild.data + "SelectedNodeID"; //Get a unique cookie name based on ComlawID
         var selectedNodeID = getCookie(COOKIE_NAME_SELECTED_NODE_ID);
         var selectedNode = document.getElementById(selectedNodeID);
         if (selectedNode != null) {
             selectedNode.scrollIntoView(true);
         }
     }

    function GetParentByTagName(parentTagName, childElementObj) {
        var parent = childElementObj.parentNode;
        while (parent.tagName != null && parent.tagName.toLowerCase() != parentTagName.toLowerCase()) {
            parent = parent.parentNode;
        }
        return parent;
    }

    function TreeviewExpandNode(treeViewId, selectedNode) {
        var relatedNodes = new Array();

        selectedNode = document.getElementById(selectedNode);
        selectedNode = GetParentByTagName("div", selectedNode); //get parent div

        //recursive loop to get all <a> elements that link back to the top node
        while (selectedNode != null && selectedNode.id != null) {
            var item = document.getElementById(selectedNode.id.substr(0, selectedNode.id.length - 5)); //Removes the 'Nodes' text from div id and gets the <a> element with the id that owns the div
            if (item != null) {
                relatedNodes.push(item);
            }
            selectedNode = GetParentByTagName("div", selectedNode);
        }

        relatedNodes.reverse();

        for (i = 0; i < relatedNodes.length; i++) {
            if (GetParentByTagName("table", relatedNodes[i]).nextSibling.style.display == "none") { //Toggle the top table element if its hidden
                eval(relatedNodes[i].href);
            }
        }
    }

    //Function from http://pushpontech.wordpress.com/2007/06/06/client-side-expandcollapse-all-nodes-for-aspnet-20-treeview/
    function TreeviewExpandCollapseAll(treeViewId, expandAll) {
        var COOKIE_NAME_EXPAND_ALL = document.getElementById("MainContent_ctl17_lblComlawID").firstChild.data + "ExpandAll"; //Get a unique cookie name based on ComlawID

        if (expandAll) {
            setCookie(COOKIE_NAME_EXPAND_ALL, "true");  
        }
        else {
            setCookie(COOKIE_NAME_EXPAND_ALL, "");  
        }
        
        var displayState = (expandAll == true ? "none" : "block");
        var treeView = document.getElementById(treeViewId);
        if (treeView) {
            var treeLinks = treeView.getElementsByTagName("a");
            var nodeCount = treeLinks.length;
            var flag = true; 
            for (i = 0; i < nodeCount; i++) {
                if (treeLinks[i].firstChild.tagName) {
                    if (treeLinks[i].firstChild.tagName.toLowerCase() == "img") {
                        var node = treeLinks[i];
                        var level = parseInt(treeLinks[i].id.substr(treeLinks[i].id.length - 1), 10);
                        var childContainer = GetParentByTagName("table", node).nextSibling; 
                        if (flag) {
                            if (childContainer.style.display == displayState) {
                                TreeView_ToggleNode(eval(treeViewId + "_Data"), level, node, '', childContainer);
                            }
                            flag = false;
                        }
                        else {
                            if (childContainer.style.display == displayState)
                                TreeView_ToggleNode(eval(treeViewId + "_Data"), level, node, '', childContainer);
                        }
                    }
                }
            } //for loop ends
            if (expandAll) {
                $('#MainContent_ctl17_divLeftBottom').show();
                    
                if($('#MainContent_ctl17_divLeftBottom').is(":visible"))
                {
                    $('#MainContent_ctl17_aHideShowTOC').html('HIDE');                            
                }
                else
                {
                        $('#MainContent_ctl17_aHideShowTOC').html('SHOW');
                }
                if ($('#MainContent_ctl17_aHideShowTOC').length > 0)
                {
                    $('#MainContent_ctl17_aHideShowTOC').attr('href', '');
                }               
                    
            }
        }

        if (expandAll) {
            ScrollIntoSelectedNode(); 
        } 
    }
    
    //Cookie related methods

    function getCookieVal(offset) {
        var endstr = document.cookie.indexOf(";", offset);
        if (endstr == -1) {
            endstr = document.cookie.length;
        }
        return unescape(document.cookie.substring(offset, endstr));
    }

    function getCookie(name) {
        var arg = name + "=";
        var alen = arg.length;
        var clen = document.cookie.length;
        var i = 0;
        while (i < clen) {
            var j = i + alen;
            if (document.cookie.substring(i, j) == arg) {
                return getCookieVal(j);
            }
            i = document.cookie.indexOf(" ", i) + 1;
            if (i == 0) break;
        }
        return "";
    }


    function setCookie(name, value) {
        document.cookie = name + "=" + value;
    }

    //End Cookie related methods
    $(function () {
        $('#MainContent_ctl17_trTOC').find('.tree').find('a').click(function (evt) {
            evt.preventDefault();
            var obj = $(this),
            getHref = obj.attr("href").split("#")[1],
            offsetSize = 160;
            if (window.innerWidth < 1280)
            {
                offsetSize = offsetSize + 450;
            }
            if ($("[name*='" + getHref + "']").length > 0)
            {
                $(window).scrollTop($("[name*='" + getHref + "']").offset().top - offsetSize);
            }
            else {
                window.location.href = obj.attr("href");
            }
            
        });
    });
    $(function () {
        $('#MainContent_ctl17_divLeftBottomExpand').click(function (evt) {
            evt.preventDefault();
            $('#MainContent_ctl17_divLeftBottom').slideToggle(
                    function ()
                    {
                        if($('#MainContent_ctl17_divLeftBottom').is(":visible"))
                        {
                            $('#MainContent_ctl17_aHideShowTOC').html('HIDE');                            
                        }
                        else
                        {
                             $('#MainContent_ctl17_aHideShowTOC').html('SHOW');
                        }
                        $('#MainContent_ctl17_aHideShowTOC').attr('href', '');
                    });
        });
    });

    function displayContents(show){

        if(show == true){
            $('#MainContent_ctl17_divLeftBottom').show();
        }
        else
        {
            $('#MainContent_ctl17_divLeftBottom').hide();
        }
                    
        if($('#MainContent_ctl17_divLeftBottom').is(":visible"))
        {
            $('#MainContent_ctl17_aHideShowTOC').html('HIDE');                            
        }
        else
        {
                $('#MainContent_ctl17_aHideShowTOC').html('SHOW');
        }
        
       

    }

</script>


