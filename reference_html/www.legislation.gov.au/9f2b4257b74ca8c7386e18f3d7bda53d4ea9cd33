                            $('#MainContent_ucLegItemPane_ucSunsetHistoryViewer_spExansionSym').html('+');
                        }
                        $('#MainContent_ucLegItemPane_ucSunsetHistoryViewer_aExpandSunset').attr('href', '');
                    });
            });
    });
</script>




