    <script>
            $(function () {
                if (window.innerWidth <= 1280) {
                    $('#MainContent_leftDetailMeta').trigger("sticky_kit:detach");
                    //alert('here1');
                    $('#MainContent_divLeftBottom').stick_in_parent({ sticky_class: 'tocStikyNow', parent: '.page-container' });
                    if (typeof displayContents === 'function')
                    {
                        displayContents(false);
                    }
                } // Set sticky for mobile device
                else
                {
                    $('#MainContent_divLeftBottom').trigger("sticky_kit:detach");
                    $('#MainContent_leftDetailMeta').stick_in_parent({ sticky_class: 'has-sticky-now' });
                    if (typeof displayContents === 'function') {
                        displayContents(true);
                    }
                } // Set sticky for non mobile device
            });


            $(window).resize(function () {
                if ($('.tablet').length > 0 || $('.mobile').length > 0 ||  $('.desktop').length > 0 ) {
                        $('#MainContent_leftDetailMeta').trigger("sticky_kit:detach");
                    //alert('here1');
                    $('#MainContent_divLeftBottom').stick_in_parent({ sticky_class: 'tocStikyNow', parent: '.page-container' });
                    if (typeof displayContents === 'function') {
                        displayContents(false);
                    }
                } // Set sticky for mobile device
                else
                {
                    $('#MainContent_divLeftBottom').trigger("sticky_kit:detach");
                    $('#MainContent_leftDetailMeta').stick_in_parent({ sticky_class: 'has-sticky-now' });
                    if (typeof displayContents === 'function') {
                        displayContents(true);
                    }
                    
                } // Set sticky for non mobile device
            });
            
        </script>
