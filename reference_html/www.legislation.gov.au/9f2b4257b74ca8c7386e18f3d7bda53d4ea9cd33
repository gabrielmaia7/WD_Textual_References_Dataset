<script type="text/javascript">
    function ClientNodeClicked(sender, eventArgs) {
        var node = eventArgs.get_node();
        node.toggle();
    }

    

    $(function(){
        $('#MainContent_ucLegItemPane_ucSunsetHistoryViewer_divSunsetExpand').click(
            function (event){
                event.preventDefault();
                $('#MainContent_ucLegItemPane_ucSunsetHistoryViewer_divSunsetInfo').slideToggle(
                    function ()
                    {
                        if($('#MainContent_ucLegItemPane_ucSunsetHistoryViewer_divSunsetInfo').is(":visible"))
                        {
                            $('#MainContent_ucLegItemPane_ucSunsetHistoryViewer_spExansionSym').html('-');                            
