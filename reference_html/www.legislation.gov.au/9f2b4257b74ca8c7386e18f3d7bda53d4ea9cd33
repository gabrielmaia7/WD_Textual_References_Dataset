<script>
    $(function () {
        var $legislation_title = $('#MainContent_ucLegItemPane_legTitle').html();
        var $legislation_title_full = '';
        if ($legislation_title.length > 99) {
            
            $legislation_title_full = "<a href='#' onclick='return false' >" + $legislation_title + "</a>";

            $legislation_title = "<a href='#' onclick='return false' >" + $legislation_title.substring(0, 99) + "..." + "</a>";
            $('.left-top h2').hover(
            function () {
                $(this).html($legislation_title_full);
            },
            function () {
                $(this).html($legislation_title);
            }
        );
        }

        $('.left-top h2').html($legislation_title);
    });

    $(function ()
    {
        $('#MainContent_ucLegItemPane_divLeftMiddle').click(
            function (event)
            {
                event.preventDefault();
                $('#MainContent_ucLegItemPane_divLeftDetails').slideToggle(
                    function ()
                    {
                        if($('#MainContent_ucLegItemPane_divLeftDetails').is(":visible"))
                        {
                            $('#MainContent_ucLegItemPane_aExpandCollapse').html('COLLAPSE');                            
                        }
                        else
                        {
                             $('#MainContent_ucLegItemPane_aExpandCollapse').html('EXPAND');
                        }
                        $('#MainContent_ucLegItemPane_aExpandCollapse').attr('href', '');
                    });
            });
    });

</script>
