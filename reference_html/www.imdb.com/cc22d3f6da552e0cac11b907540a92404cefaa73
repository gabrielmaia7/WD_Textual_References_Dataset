};
var genericEvt;
videoEvt.type = '1P-ad-autoplay-video-maybe-present';
if (window && window.origin) {
window.postMessage(videoEvt, window.origin);
if (genericEvt) {
window.postMessage(genericEvt, window.origin);
}
}
</script>
<!-- End INLINE80 -->
	


    
    
    



    
    
