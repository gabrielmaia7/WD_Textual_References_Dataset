if (ad_utils.inject_serverside_ad) {
ad_utils.inject_serverside_ad('bottom_ad', ' <script>document.defaultAd = {h:90,w:728}; document.ad = document.defaultAd;<\/script>\n<script id=\"video-event-handlers\">\n (function(window, document) {\n let SLOT_NAME = \'bottom_ad\';\n let videoTargetMap = new Map(); \n let validPlayIndicatorEvents = [\'loadstart\', \'play\'];\n\n function makeId() {\n return Math.random().toString(36).substr(2, 16);\n }\n\n function handleVideoEvent(event) {\n let videoTarget = event.target;\n let eventType = event.type;\n let timestamp = Date.now();\n\n if (videoTarget.nodeName !== \'VIDEO\' || videoTarget.loop) {\n return;\n }\n\n if (!videoTargetMap.has(videoTarget)) {\n let id = makeId();\n videoTargetMap.set(videoTarget, {\n id: id\n });\n videoTarget.id = id;\n }\n\n let videoTargetEventMap = videoTargetMap.get(videoTarget);\n videoTargetEventMap[eventType] = timestamp;\n\n if (eventType === \'ended\') {\n let firedEvents = Object.keys(videoTargetEventMap);\n let hasMatchingPlayEvent = firedEvents.some(function(evt) {\n return validPlayIndicatorEvents.includes(evt);\n });\n if (!hasMatchingPlayEvent) return;\n }\n\n let evt = {\n type: eventType,\n dispatcher: \'video-handler\',\n documentAdString: JSON.stringify(document.ad),\n slotName: SLOT_NAME,\n targetElementId: videoTargetEventMap.id,\n timestamp: timestamp\n };\n\n parent.postMessage(evt, parent.origin);\n }\n\n document.addEventListener(\'loadstart\', handleVideoEvent, true);\n document.addEventListener(\'play\', handleVideoEvent, true);\n document.addEventListener(\'pause\', handleVideoEvent, true);\n document.addEventListener(\'ended\', handleVideoEvent, true);\n })(window, document);\n<\/script>\n <!DOCTYPE html><html><body><\!-- aid : 7820954750302 : cid : 6411293630402 : template : IMDb Slot Collapser Desktop-1.21 --\><\!-- [METADATA_START] {\n \"creativeId\" : \"6411293630402\"\n \n , \"creativeHeight\": 0 \n , \"creativeWidth\": 0 \n \n \n } [METADATA_END] --\><script>document.ad={aid:\"7820954750302\",cid:\"