        <div class="article-strip__content container__inner">
        
            
            

    <h2 class="section__title">You might like</h2>
    <div class="card-group card--carousel" data-strip-label="you-might-like">
        
            <div class="carousel__button-group">
                <a href="#" class="carousel--left btn btn--style__icon slick-arrow slick-disabled" data-gtm-name="Carousel-left" aria-disabled="true" style="display: inline;">
                    <i class="btn__icon icon--arrow-left"></i>
                    <span class="btn__text">Left</span>
                </a>
                <a href="#" class="carousel--right btn btn--style__icon slick-arrow" data-gtm-name="Carousel-right" style="display: block;" aria-disabled="false">
                    <i class="btn__icon icon--arrow-right"></i>
                    <span class="btn__text">Right</span>
                </a>
            </div>
        
        <ul class="card-list card-list--type__portrait card-list--4col card-list--100 slick-initialized slick-slider">
        
            
            
            
            <div class="slick-list draggable"><div class="slick-track" style="opacity: 1; width: 4088px; transform: translate3d(0px, 0px, 0px);"><li class="card card--artwork slick-slide slick-current slick-active" data-slick-index="0" aria-hidden="false" style="width: 292px;" tabindex="0">
            



<div class="card__inner">
