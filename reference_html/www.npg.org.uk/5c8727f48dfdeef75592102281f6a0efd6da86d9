<script type="text/javascript">

	function openHelp(index) {
		//sorting out help text popover in catalogue pages
		var helpText = $('helpTextBox'+index);
		$('helpTextBox'+index).className='helpTextBox helpTextContainer';
		//helpText.setStyle('display','block');
		helpText.style.display= 'block';
		return false;
	}
	function closeHelp(index) {
		//sorting out help text popover in catalogue pages
		var helpText = $('helpTextBox'+index);
		helpText.style.display= 'none';
		return false;
	}
</script>

