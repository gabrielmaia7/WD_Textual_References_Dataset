        <img src="/assets/js/openseadragon/images/zoomin_rest.png" alt="">
        <img src="/assets/js/openseadragon/images/zoomin_hover.png" alt="">
      </div>

      <div id="osd-btn-zoom-out-1" class="osd-controls__button osd-controls__button--zoom-out" title="Zoom out" style="display: inline-block; position: relative;">
        <img src="/assets/js/openseadragon/images/zoomout_rest.png" alt="">
        <img src="/assets/js/openseadragon/images/zoomout_hover.png" alt="">
      </div>

