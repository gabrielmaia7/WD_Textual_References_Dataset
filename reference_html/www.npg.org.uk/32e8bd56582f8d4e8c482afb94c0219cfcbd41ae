<script type="text/javascript">
  (function ($) {
    $(function () {
      var mkey = 'mw00808';

      viewer1 = OpenSeadragon({
        id: "openseadragon1",
        prefixUrl: "/assets/js/openseadragon/images/",
        tileSources: "https://collectionimages.npg.org.uk/zoom/mw00808/zoomXML.dzi",
        showNavigator: false ,
        defaultZoomLevel: 0 ,
        minZoomImageRatio: 1,
                zoomInButton: "osd-btn-zoom-in-1",
        zoomOutButton: "osd-btn-zoom-out-1",
        fullPageButton: "osd-btn-full-screen-1",
        rotateRightButton: "osd-btn-rotate-right-1",
        visibilityRatio: 1,
        navImages: {
          fullpage: {
            REST: 'fullpage_rest.png',
            GROUP: 'fullpage_rest.png',
            HOVER: 'fullpage_hover.png',
            DOWN: 'fullpage_hover.png'
          },
          zoomIn: {
            REST: 'zoomin_rest.png',
            GROUP: 'zoomin_rest.png',
            HOVER: 'zoomin_hover.png',
            DOWN: 'zoomin_hover.png'
          },
          zoomOut: {
            REST: 'zoomout_rest.png',
            GROUP: 'zoomout_rest.png',
            HOVER: 'zoomout_hover.png',
            DOWN: 'zoomout_hover.png'
          },
          rotateright: {
            REST: 'rotateright_rest.png',
            GROUP: 'rotateright_rest.png',
            HOVER: 'rotateright_hover.png',
            DOWN: 'rotateright_hover.png'
          }
        }
      });

      function scrollToViewer() {
        $('html, body').animate({
          scrollTop: (viewer1.element.offsetTop +
            ($('#main').css('marginTop').slice(0, -2) | 0) +
            $('#header').height()) + 'px'
        }, 400);
      }

      function baseRectConverter(baseDims, sitterRect) {
        var sourceWidth = viewer1.source.width;
        var ratio = 1;
        var formattedDims;

        //left(0),top(1),right(2),bottom(3) -> x,y,w,h
        function formatAndScaleBaseCoords(sitterRect, ratio) {
          var scaledCoords = sitterRect.split(',')
            .map(function (coord) {
              return ((coord | 0) * ratio).toFixed(2) | 0;
            });
          return {
            x: scaledCoords[0],
            y: scaledCoords[1],
            w: scaledCoords[2] - scaledCoords[0],
            h: scaledCoords[3] - scaledCoords[1]
          };
        }

        function baseDimFormatter(baseDims) {
          var oldDims = baseDims.split(',')
            .map(function (dim) {
              return dim | 0;
            });

          return newDims = {
            width: oldDims[0],
            height: oldDims[1]
          }
        }

        formattedBaseDims = baseDimFormatter(baseDims);

        if (sourceWidth !== formattedBaseDims.width) {
          ratio = sourceWidth / formattedBaseDims.width;
        }

        return formatAndScaleBaseCoords(sitterRect, ratio);

      }

      viewer1.addHandler('open', function () {
        viewer1.customControlsContainer = [];
        viewer1.customControlsContainer[0] = document.getElementById(
          'osd-controls-1');
        beginControlsAutoHide(viewer1 );		
        if (mkey) {
          $.ajax(window.location.origin + '/collections/search/sitterRects/' + mkey)
            .done(function (res) {
              var $viewerEl = $('#openseadragon1');
              var $nav = $('#navigation');
              var rects = JSON.parse(res).rects;
	  		      var $list = $('#sitter-list, #artist-list');
	 		        var $links = $list.find('a[data-linkid]');			  
              var rectIds = Object.getOwnPropertyNames(rects);
              var osdRects = {};
              var $wwTextbox = $('<div id="ww-textbox"></div>');
              var stickyHeadID;

			        $list.addClass('show-identify');

              $viewerEl.append($wwTextbox);

              for (var i = 0; i < rectIds.length; i++) {
                var cvRect = baseRectConverter(rects[rectIds[i]].basedims, rects[rectIds[i]].sitterrect);
                var osdRect = viewer1.viewport.imageToViewportRectangle(cvRect.x,
                  cvRect.y, cvRect.w, cvRect.h);
                viewer1.addOverlay({
                  element: $('<div id="ww-overlay-' + rectIds[i] +
                    '" class="ww-overlay" data-linkid="' + rectIds[i] + '"></div>')[0],
                  location: osdRect,
                });
                osdRects[rectIds[i]] = osdRect;
              }

              function setStickyHead() {
			  	var stickyHeadEl;
                if (stickyHeadID) {
				  stickyHeadEl = $('#ww-overlay-' + stickyHeadID);
                  $('.ww-overlay').css('opacity', 0).removeClass('sticky');
                  stickyHeadEl.css('opacity', 1).addClass('sticky');
                  $wwTextbox.html('<span><strong>' + rects[stickyHeadID].prefname + '</strong> ' + rects[stickyHeadID].occupation + '</span> <a href="' + rects[stickyHeadID].sitterURL + '">More...</a>');
                  $wwTextbox.show();
                }
              }

              $viewerEl.on('mouseenter', '.ww-overlay', function () {
                var $this = $(this);
                var rectRef = $this.data('linkid');
				// No enter event for sticky head
				if ($this.hasClass('sticky')) return;
                $('.ww-overlay').css('opacity', 0);
                $this.css('opacity', 1);
                $wwTextbox.html('<strong>' + rects[rectRef].prefname + '</strong> ' + rects[rectRef].occupation);
                $wwTextbox.show();
              }).on('mouseleave', '.ww-overlay', function () {
                $(this).css('opacity', 0);
                $wwTextbox.hide();
                setStickyHead();
              }).on('click', '.ww-overlay', function () {
                stickyHeadID = $(this).data('linkid');
                setStickyHead();
              }).on('click', function(e) {
                if (e.target.tagName === 'CANVAS') {
                  $('.ww-overlay').css('opacity', 0);
                  $wwTextbox.hide();
                }
              });

              $links.on('click', function (e) {
                var $this = $(this);
                var rectRef = $this.data('linkid');
                var imageRect = baseRectConverter(rects[rectRef].basedims, rects[rectRef].sitterrect);

                // This will be cached for sticky behaviour
                stickyHeadID = rectRef;
                setStickyHead();

                if (imageRect.h > $viewerEl.height()) {
                  imageRect.h *= 1.25;
                  imageRect.y *= 0.25;
                }
                if (imageRect.w > $viewerEl.width()) {
                  imageRect.w *= 1.25;
                  imageRect.x *= 0.25;
                }

                var vpRect = viewer1.viewport.imageToViewportRectangle(imageRect
                  .x, imageRect.y, imageRect.w, imageRect.h);

                viewer1.viewport.fitBoundsWithConstraints(vpRect);
                scrollToViewer();
                e.preventDefault();
              });
            });
        }
      });

      viewer1.addHandler('container-enter', function () {
        abortControlsAutoHide(viewer1 );
      });

      viewer1.addHandler('container-exit', function () {
        beginControlsAutoHide(viewer1 );
      });
    });
  }(jQuery))
</script>
